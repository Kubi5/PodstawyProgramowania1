#include <stdio.h>

int main()
//alfabet
{
char k;
printf("ALFABET, podaj literę z zakresu t,b,s,o,p\n");
scanf("%c",&k);


        if(k=='t' || k=='T') {
        printf("TWOJE USTA DZIŚ WYRZEKŁY ME IMIE");
        }

        else if(k=='b' || k=='B'){
        printf("BY ŁOWIĆ SERCA");
        }

        else if(k=='s' || k=='S'){
        printf("SZUKAŁ LUDZI GOTOWYCH PÓJŚĆ ZA NIM");
        }

        else if(k=='o' || k=='O'){
        printf("O PANIE TO TY NA MNIE SPOJRZAŁEŚ");
        }

        else if(k=='p' || k=='P') {
        printf("PAN KIEDYŚ STANĄŁ NAD BRZEGIEM");
        }
        else
        printf("Nie podano litery z zakresu");

return 0;
}

//kalkulator
{
printf("WITAMY W PROGRAMIE KALKULATOR!!\n");
printf("Podaj operację jaka ma zostać wykonana(+,-,*,/):\n");
char a;
float x,y;
scanf("%c",&a);

    switch(a)
    {
        case '+':
        {
        printf("DODAWANIE\n");
        printf("Wpisz liczby:\n");
        scanf("%f %f",&x,&y);
        printf("Suma wynosi: %0.2f ",x+y);
        break;
        }
        {
        case '-':
        printf("ODEJMOWANIE\n");
        printf("Wpisz liczby:\n");
        scanf("%f %f",&x,&y);
        printf("Różnica wynosi: %0.2f", x-y);
        break;
        }
        {
        case '*':
        printf("MNOŻENIE\n");
        printf("Wpisz liczby:\n");
        scanf("%f %f",&x,&y);
        printf("Iloczyn wynosi: %0.2f ",x*y);
        break;
        }
        {
        case '/':
        printf("DZIELENIE\n");
        printf("Wpisz liczby:\n");
        scanf("%f %f",&x,&y);
        printf("Iloraz wynosi: %0.2f ",x/y);
        break;
        }
        default:
        printf("NIE PODANOWO WŁAŚCIWEGO ZNAKU");
    }

   return 0;
}



{
//PROGRAM SUMUJĄCY LICZBY OD 30 do 100
int i=30;
int wynik=0;

    while (i<=100)
    {
    wynik+=i;
    ++i;
    }
    printf("%d\n", wynik);

return 0;

}


{
//liczby podzielne przez 6 mniejsze niż 100
int i=1;
int wynik;


    do
    {
    wynik=6*i;
    printf("%i\n",wynik);
    ++i;
    }
    while(i*6<100);


return 0;
}


{
//Czy dana liczba jest pierwsza?
int a;
int i;

    scanf("%i",&a);
    for(i=2;a>i;++i)
    {
        if (a%i==0)
        {
        printf("Liczba NIE JEST liczbą pierwszą");
        return 0;
        }
    }

    printf("Liczba JEST liczbą pierwszą");

return 0;
}


{
//tabliczka mnożenia
int i=0;
int n=0;
int wynik;
    for(i=0; i<=10;i++)
    {
        for(n=0;n<=10;n++)
        printf("%i*%i=%i   ",i,n,i*n);
        if(n%2==0)
            printf("\n\n");
    }

return 0;
}

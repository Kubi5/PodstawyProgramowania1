void tekstydwa(char* a[100],char* b[100]){
int i;
printf("[%s]%s",a,b);
}

int main()
{
char* a[100];
char* b[100];
fgets(a,100,stdin);
fgets(b,100,stdin);

tekstydwa(a,b);

return 0;
}

int licznik=1;
void komunikat(){
printf("%i. Jest to stały komunikat zmienia się tylko liczba\n",licznik);
licznik++;
}
int main()
{
int licznik;
komunikat();
komunikat();
komunikat();
komunikat();
komunikat();
komunikat();

return 0;
}

int silnia(int a){
int i;
int wynik=1;

    for(i=a;i>=1;--i){
    wynik=wynik*i;
    }

    return wynik;
}

int main()
{
int b;
printf("Podaj liczbę funkcja liczy silnie\n");
scanf("%i",&b);
printf("Silnia z liczby %i wynosi: %i",b,silnia(b));

return 0;
}


void podwojenie(int tab[]){
int i;

    printf("Podwojona tablica:\n");
    for(i=0;i<4;++i){
    printf("%i ",tab[i]*2);
    }
}
int main()
{
int tab[4]={1,2,3,4};
int i;
podwojenie(tab);

    printf("\nStara tablica wyglądała tak:\n");
    for(i=0;i<4;++i){
    printf("%i ",tab[i]);
    }

return 0;
}

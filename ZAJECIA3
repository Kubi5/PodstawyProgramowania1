#include <stdio.h>
#include <stdarg.h>
#include <time.h>


int szescian(int a){
return a*a*a;
}


int main()
{
int a;
scanf("%i",&a);
printf("Sześcian liczby wynosi %i",szescian(a));


return 0;

}

int average(int a,int b,int c,int d, int e){

return (a+b+c+d+e)/5;
}

int main()
{
int a,b,c,d,e;
int tab[5]={1,3,5,7,9};

a=tab[0];
b=tab[1];
c=tab[2];
d=tab[3];
e=tab[4];

printf("Średnia liczb w tablicy wynosi: %i",average(a,b,c,d,e));

return 0;
}

float podwojenie(float a){
return a*2;
}
int main()
{
float a;
printf("Wpisz liczbę:\n");
scanf("%f",&a);
printf("Podwojona liczba wynosi %.2f, twoja oryginalna liczba wynosi %.2f",podwojenie(a),a);

return 0;
}

